{
  "name": "pona-discord-application",
  "version": "1.0.0",
  "description": "A useful discord bot",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "bin": "./dist/cli.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "dotenvx run -f .env.development -- cross-env CLUSTER=false tsx watch src/index.ts",
    "type-check": "npx tsc --noEmit",
    "start-shard": "dotenvx run -f .env.production -- cross-env CLUSTER=true node -r ./tsconfig-paths.js dist/shard.js",
    "start": "dotenvx run -f .env.production -- cross-env CLUSTER=false node -r ./tsconfig-paths.js dist/index.js",
    "build": "npx tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ponlponl123/Pona-Discord-Application.git"
  },
  "keywords": [
    "Ponlponl123",
    "Pona",
    "Discord",
    "Application",
    "Bot",
    "Dashboard",
    "Useful",
    "Helpful"
  ],
  "author": "Ponlponl123",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ponlponl123/Pona-Discord-Application/issues"
  },
  "homepage": "https://github.com/ponlponl123/Pona-Discord-Application#readme",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@dotenvx/dotenvx": "^1.21.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/lodash": "^4.17.12",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord-hybrid-sharding": "^2.2.3",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "ffmpeg-static": "^5.2.0",
    "ioredis": "^5.4.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.3",
    "redis": "^4.7.0",
    "socket.io": "^4.7.5",
    "socket.io-redis": "^5.4.0",
    "sodium-native": "^4.2.1",
    "tsconfig-paths": "^4.2.0",
    "xmlhttprequest-ts": "^1.0.1",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "@types/socket.io-redis": "^1.0.27",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.4.0",
    "globals": "^15.8.0",
    "pkgroll": "^2.5.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^7.16.0"
  }
}
